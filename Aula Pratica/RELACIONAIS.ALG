algoritmo "Relacionais"
var
   A, B, C: Inteiro
inicio
      A <- 2
      B <- 3
      C <- 5
      Escreval (A>B)
      Escreval (A<B)
      Escreval (A=B)
      Escreval (A<>B)
      Escreval (A<=B)
      Escreval (A>=B)
      Escreval (C=A+B)
      Escreval (C>=B^A)
      Escreval (B<=C%2)
      Escreval (1=C%2)
fimalgoritmo