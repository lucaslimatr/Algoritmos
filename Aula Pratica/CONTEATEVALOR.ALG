algoritmo "ConteAteValor"
var
   contador, valor: Inteiro
inicio
      contador <- 0
      Escreva ("Quer contar até quanto? ")
      Leia (Valor)
      Enquanto (contador <= valor) faca
               EscrevaL (contador)
               contador <- contador + 1
      FimEnquanto
      Escreva ("Terminei de contar")
fimalgoritmo